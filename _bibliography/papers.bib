@incollection{cano2018,
  title = {ABTS/TEAC (2,2'-Azino-Bis(3-Ethylbenzothiazoline-6-Sulfonic Acid)/Trolox®-Equivalent Antioxidant Capacity) Radical Scavenging Mixed-Mode Assay},
  booktitle = {Measurement of Antioxidant Activity & Capacity},
  author = {Cano, Antonio and Arnao, Marino B.},
