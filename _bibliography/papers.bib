@article{fernandez-trujillo2000,
  title = {Interactions among Cooling, Fungicide and Postharvest Ripening Temperature on Peaches},
  author = {Fernández-Trujillo, J. Pablo and Cano, Antonio and Artés, Francisco},
  year = {2000},
