@article{artes1998,
  title = {Physiological Responses of Tomato Fruit to Cyclic Intermittent Temperature Regimes},
  author = {Artés, F. and García, F. and Marquina, J. and Cano, A. and Fernández-Trujillo, J. P.},
  year = {1998},
  journal = {Postharvest Biology and Technology},
  volume = {14},
